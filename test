	RightLeg.Anchored=true RightLeg.Material='SmoothPlastic' RightLeg.Transparency=0 RightLeg.Reflectance=0   RightLeg.Size=Vector3.new(1,2,1) RightLeg.Parent=model
	LeftLeg.Anchored=true LeftLeg.Material='SmoothPlastic' LeftLeg.Transparency=0 LeftLeg.Reflectance=0  LeftLeg.Size=Vector3.new(1,2,1) LeftLeg.Parent=model
