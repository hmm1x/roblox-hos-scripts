local soundtrack
coroutine.resume(coroutine.create(function()
	while true do
		soundtrack = soundinpart.TimePosition
		wait(1)
	end
end))

function refit_rem()
	refitremote = Instance.new("RemoteEvent",remotething) refitremote.Name='2'
	refitremote.OnServerEvent:Connect(function(yes,bye)
		Head:Destroy() Torso:Destroy() RightLeg:Destroy() LeftLeg:Destroy() RightArm:Destroy() LeftArm:Destroy()	model:Destroy() soundinpart:Destroy()
	end)

end

