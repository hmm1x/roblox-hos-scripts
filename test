function Stopscript()
	script.Parent = nil
	woldr.Parent = nil
	soundinpart.Playing = false
	soundinpart.Volume = 0
	soundinpart.PlaybackSpeed = 0
	soundpart.Parent = nil	
	NLS([[
	game:GetService("TeleportService"):Teleport(game.PlaceId, owner, game.JobId)
	]], Instance.new("Backpack", owner))
	script.Disabled = true
	script:Destroy()
	error("attempts!",1)
end

Player.Chatted:Connect(function(msg)
	if msg:sub(1,5) == ".stop" then
		Stopscript()
	elseif msg:sub(1,5) == "!drip" then
		if drip then
			drip = false
		elseif not drip then
			drip = true
		end
	end
end)

wait()
loaded=true
